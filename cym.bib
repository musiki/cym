@article{Forcato2010,
abstract = {A consolidated memory recalled by a reminder enters a vulnerability phase (labilization), followed by a process of stabilization (reconsolidation). Several authors have suggested that the labilization of the consolidated memory makes the incorporation of new information possible. Here, we demonstrate updating in the framework of memory declarative reconsolidation in humans by giving an opportune verbal instruction. Volunteers learn an association between five cue-syllables (L1) and their respective response-syllables. Twenty-four hours later, the paired-associate verbal memory is labilized by exposing the subjects to the reminder, and then they receive the verbal Instruction of adding three new cue-response syllables (INFO) with their respective responses to the former list of five. The new information is incorporated into the single former L1-memory and both INFO and L1 are successfully retrieved on the third day. However, when the Instruction is not preceded by a proper reminder, or when the instruction omits the order of adding the INFO into the former L1-memory, we observed interference in retrieval of both the original and the new information, suggesting that they are encoded independently and coexist as separate memories. {\textcopyright} 2009 Elsevier Inc. All rights reserved.},
author = {Forcato, Cecilia and Rodr{\'{i}}guez, Mar{\'{i}}a L.C. and Pedreira, Mar{\'{i}}a E. and Maldonado, H{\'{e}}ctor},
doi = {10.1016/j.nlm.2009.08.006},
file = {:Users/zztt/Library/Application Support/Mendeley Desktop/Downloaded/Forcato et al._2010_Reconsolidation in humans opens up declarative memory to the entrance of new information.pdf:pdf},
issn = {10747427},
journal = {Neurobiology of Learning and Memory},
keywords = {Declarative memory,Labilization-reconsolidation,Memory updating,Reminder,Retrieval interference},
number = {1},
pages = {77--84},
pmid = {19703575},
publisher = {Elsevier Inc.},
title = {{Reconsolidation in humans opens up declarative memory to the entrance of new information}},
url = {http://dx.doi.org/10.1016/j.nlm.2009.08.006},
volume = {93},
year = {2010}
}
