journal = {Educational Technology},
number = {88},
pages = {309},
title = {{Artificial Intelligence , Education and Music composition by novices}},
url = {http://mcl.open.ac.uk/Hsp/uploads/Simon Holland PhD Thesis.pdf},
year = {1989}
